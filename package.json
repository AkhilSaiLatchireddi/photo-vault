{
  "name": "photovault",
  "version": "1.0.0",
  "description": "Personal photo storage app with AWS S3 backend",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:react\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:react": "cd frontend-react && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "npm run build:react && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:react": "cd frontend-react && npm run build",
    "build:backend": "cd backend && npm run build",
    "start": "npm run start:backend",
    "start:backend": "cd backend && npm start",
    "install:all": "npm install && cd frontend && npm install && cd ../frontend-react && npm install && cd ../backend && npm install",
    "test": "npm run test:react && npm run test:backend",
    "test:frontend": "cd frontend && npm test",
    "test:react": "cd frontend-react && npm run lint",
    "test:backend": "cd backend && npm test"
  },
  "keywords": [
    "photos",
    "storage",
    "aws",
    "s3",
    "personal",
    "cloud",
    "memories"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "workspaces": [
    "frontend",
    "frontend-react",
    "backend"
  ]
}
