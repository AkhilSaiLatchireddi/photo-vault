{
  "name": "photovault-lambda-backend",
  "version": "1.0.0",
  "description": "PhotoVault Lambda Backend - Production Ready",
  "main": "lambda.js",
  "scripts": {
    "build": "tsc",
    "start": "node -r dotenv/config index.js",
